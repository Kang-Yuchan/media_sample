<h2>Show admin user</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @admin_user.email %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @admin_user.name %>
  </li>

  <li>
    <strong>Status:</strong>
    <%= Status.get(@admin_user.status).text %>
  </li>

</ul>

<%= link "Edit", to: admin_admin_user_path(@conn, :edit, @admin_user) %>
<%= link "Back", to: admin_admin_user_path(@conn, :index) %>
