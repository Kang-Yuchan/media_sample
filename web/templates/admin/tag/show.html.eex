<h2>Show tag</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @tag.name %>
  </li>

  <li>
    <strong>Status:</strong>
    <%= Status.get(@tag.status).text %>
  </li>

</ul>

<%= link "Edit", to: admin_tag_path(@conn, :edit, @conn.assigns.locale, @tag) %>
<%= link "Back", to: admin_tag_path(@conn, :index, @conn.assigns.locale) %>
